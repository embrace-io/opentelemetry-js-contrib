{
  "name": "@opentelemetry/instrumentation-react-native-redux",
  "version": "0.1.0",
  "description": "OpenTelemetry instrumentation for 'Redux' actions applied to React Native applications",
  "keywords": [
    "redux",
    "react-native",
    "instrumentation",
    "nodejs",
    "opentelemetry",
    "profiling",
    "tracing"
  ],
  "homepage": "https://github.com/open-telemetry/opentelemetry-js-contrib/tree/main/plugins/node/instrumentation-react-native-redux#readme",
  "license": "Apache-2.0",
  "author": "OpenTelemetry Authors",
  "bugs": {
    "url": "https://github.com/open-telemetry/opentelemetry-js-contrib/issues"
  },
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src/**/*.js",
    "build/src/**/*.js.map",
    "build/src/**/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": "open-telemetry/opentelemetry-js-contrib",
  "scripts": {
    "clean": "rimraf build/*",
    "compile": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "lint:readme": "node ../../../scripts/lint-readme.js",
    "precompile": "tsc --version && lerna run version:update --scope @opentelemetry/instrumentation-react-native-redux --include-dependencies",
    "prewatch": "npm run precompile",
    "prepublishOnly": "npm run compile",
    "test": "nyc mocha",
    "test:watch": "nyc mocha -w",
    "test-all-versions": "tav",
    "version:update": "node ../../../scripts/version-update.js",
    "watch": "tsc -w",
    "postinstall": "rm -rf ./node_modules/react"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.0.0",
    "react-native": "*",
    "@reduxjs/toolkit": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/register": "^7.24.6",
    "@opentelemetry/api": "^1.0.0",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/mocha": "8.2.3",
    "@types/node": "18.6.5",
    "@types/sinon": "10.0.18",
    "babel-plugin-react-native-web": "^0.19.12",
    "jsdom": "^15.2.1",
    "jsdom-global": "^3.0.2",
    "metro-react-native-babel-preset": "^0.77.0",
    "mocha": "10.0.0",
    "nyc": "15.1.0",
    "react-dom": "~18.2.0",
    "react-native": "^0.74.0",
    "react-native-web": "^0.19.12",
    "sinon": "15.2.0",
    "ts-mocha": "10.0.0",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=14"
  }
}
